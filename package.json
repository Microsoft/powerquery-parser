{
  "name": "powerquery-parser",
  "version": "0.0.10",
  "description": "A parser for the Power Query langauge.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "grapheme-splitter": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "files": [
    "bundle/*",
    "lib/common/*",
    "lib/localization/*",
    "lib/lexer/*",
    "lib/parser/*",
    "lib/index.d.ts",
    "lib/index.js",
    "lib/index.js.map",
    "lib/jobs.d.ts",
    "lib/jobs.js",
    "lib/jobs.js.map"
  ],
  "scripts": {
    "build": "node_modules\\.bin\\tsc.cmd",
    "build-all": "npm run-script build & npm run-script build-webpack",
    "build-webpack": "npm run-script build-webpack-prod & npm run-script build-webpack-dev",
    "build-webpack-prod": "webpack --config webpack/prod.config.js",
    "build-webpack-dev": "webpack --config webpack/dev.config.js",
    "test": "mocha --reporter mocha-multi-reporters -r ts-node/register src/test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/powerquery-parser.git"
  },
  "author": "Jordan Bolton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/powerquery-parser/issues"
